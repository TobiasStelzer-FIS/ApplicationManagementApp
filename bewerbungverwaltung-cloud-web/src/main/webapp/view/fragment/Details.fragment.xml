		<f:Form>
			<f:layout>
				<f:ResponsiveGridLayout/>
			</f:layout>
			<f:FormContainer>
				<f:title>
					<core:Title text="Persönliche Daten"/>
				</f:title>
				<f:FormElement label="Nachname">
					<Text text="{testModel>BewerberDetails/Nachname}"/>
				</f:FormElement>
				<f:FormElement label="Vorname">
					<Text text="{testModel>BewerberDetails/Vorname}"/>
				</f:FormElement>
				<f:FormElement label="Straße">
					<Text text="{testModel>BewerberDetails/StrasseHnr}"/>
				</f:FormElement>
				<f:FormElement label="PLZ / Ort">
					<Text text="{testModel>BewerberDetails/Postleitzahl} {testModel>BewerberDetails/Ort}"/>
				</f:FormElement>
			</f:FormContainer>
			<f:FormContainer formElements="{path: 'testModel>BewerbungStelleDetails/results'}">
				<f:title>
					<core:Title text="Beworben als"/>
				</f:title>
				<f:FormElement>
					<Text text="• {testModel>StelleDetails/Bezeichnung}" class="sapUiLargeMarginBegin"/>
				</f:FormElement>
			</f:FormContainer>
			<f:FormContainer>
				<f:title>
					<core:Title text="Kontaktinformationen"/>
				</f:title>
				<f:FormElement label="Email">
					<Text text="{testModel>BewerberDetails/Email}"/>
				</f:FormElement>
				<f:FormElement label="Telefon">
					<Text text="{testModel>BewerberDetails/Telefonnummer}"/>
				</f:FormElement>
				<f:FormElement label="Mobil">
					<Text text="{testModel>BewerberDetails/Handynummer}"/>
				</f:FormElement>
			</f:FormContainer>
			<f:FormContainer formElements="{path: 'testModel>BewerbungQuelleDetails/results'}">
				<f:title>
					<core:Title text="Quellen"/>
				</f:title>
				<f:FormElement>
					<Text text="• {testModel>QuelleDetails/Bezeichnung}" class="sapUiLargeMarginBegin"/>
				</f:FormElement>
			</f:FormContainer>
		</f:Form>
